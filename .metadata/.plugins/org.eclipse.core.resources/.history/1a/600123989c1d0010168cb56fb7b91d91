package entities;

import entities.enums.Colors;

public class Shape {
	private Colors color;

	public Shape() {
		
	}
	
	public Shape(Colors color) {
		super();
		this.color = color;
	}

	public Colors getColor() {
		return color;
	}

	public void setColor(Colors color) {
		this.color = color;
	}
	
	
	public Double area() {
		return 3.0 + 2 ;
	}
}
